<p id="notice"><%= notice %></p>



<h1>AIRTEL PACKS</h1>

<table>
  <thead>
    <tr>
      <th>Region id</th>
      <th>Operator id</th>
      <th>Connection type</th>
      <th>Pack type</th>
      <th>Price</th>
      <th>Offer</th>
      <th>Validity</th>
      <th>Description</th>
      <th>Tag</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @airtels.each do |pack| %>
      <tr>
        <td><%= pack.region_id %></td>
        <td><%= pack.operator_id %></td>
        <td><%= pack.connection_type %></td>
        <td><%= pack.pack_type %></td>
        <td><%= pack.price %></td>
        <td><%= pack.offer %></td>
        <td><%= pack.validity %></td>
        <td><%= pack.description %></td>
        <td><%= pack.tag %></td>
        <td><%= link_to 'Show', pack %></td>
        <td><%= link_to 'Edit', edit_pack_path(pack) %></td>
        <td><%= link_to 'Destroy', pack, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Pack', new_pack_path %>
